        <TextView
            android:id="@+id/tv_aba_inicio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/inicio"
            android:layout_margin="10dp"
            android:background="@drawable/formato_bandeirinha_add"/>

        <TextView
            android:id="@+id/tv_aba_servicos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/servico"
            android:layout_margin="10dp"/>

        <TextView
            android:id="@+id/tv_aba_produtos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/produtos"
            android:layout_margin="10dp"/>

        <TextView
            android:id="@+id/tv_aba_feedback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/feedbacks"
            android:layout_margin="10dp"/>

    </LinearLayout>


