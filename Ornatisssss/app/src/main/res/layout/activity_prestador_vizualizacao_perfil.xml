        <androidx.cardview.widget.CardView
            android:id="@+id/card_menu"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_centerInParent="true"
            app:cardCornerRadius="100dp">

            <ImageView
                android:id="@+id/iv_foto_perfil_estabelecimento"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:scaleType="fitXY"
                android:src="@drawable/ellipse70" />

        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/tv_nome_do_negocio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/card_menu"
            android:layout_centerInParent="true"
            android:layout_marginTop="1dp"
            android:text="Beauty Hair"
            android:textAlignment="center"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/avalicao_salao"
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:layout_below="@id/tv_nome_do_negocio"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:layout_marginTop="3dp"
            android:drawableStart="@drawable/img_estrela"
            android:drawablePadding="7dp"
            android:paddingTop="3dp"
            android:text="4.5" />


    </RelativeLayout>

    //ABAS PARA MUDAR DE TELA
    <LinearLayout
        android:layout_width="match_parent"
